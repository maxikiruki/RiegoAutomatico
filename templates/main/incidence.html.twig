{% extends 'base.html.twig' %}

{% block title %}
REPORTAR INCIDENCIA
{% endblock %}


{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

{{ form_start(form) }}
    {{ form_widget(form) }}
    <button id="guardar">{{ button_label|default('Enviar') }}</button>
{{ form_end(form) }}

<button id="salir" onclick="window.location.href='{{ path('main') }}'">Cancelar</button>
{% endblock %}
